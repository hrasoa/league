[ignore]

[include]

[libs]
flow-typed

[lints]

[options]
module.file_ext=.js
module.file_ext=.scss
module.file_ext=.json
module.file_ext=.graphql
module.ignore_non_literal_requires=true
module.name_mapper='.*loadable\.json$' -> '<PROJECT_ROOT>/flow-stubs/loadable.json'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/flow-stubs/css-module.js.flow'
module.name_mapper.extension='graphql' -> '<PROJECT_ROOT>/flow-stubs/graphql.js.flow'
; module.name_mapper='.*\(svg\|png\|jpg\|gif\)$' -> '<PROJECT_ROOT>/flow-stubs/url-loader.js.flow'

[strict]

[untyped]
; https://github.com/facebook/draft-js/issues/1496#issuecomment-353895318
.*/node_modules/draft-js/lib/DraftEditor.react.js.flow
; https://github.com/facebook/draft-js/issues/1621
.*/node_modules/draft-js/lib/ContentBlock.js.flow
.*/node_modules/draft-js/lib/ContentBlockNode.js.flow
.*/node_modules/draft-js/lib/DraftEditorLeaf.react.js.flow
.*/node_modules/graphql/jsutils/keyValMap.js.flow
.*/node_modules/graphql/utilities/buildASTSchema.js.flow
